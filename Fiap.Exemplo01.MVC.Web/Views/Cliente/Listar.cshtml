@model List<Fiap.Exemplo01.MVC.Web.Models.Cliente>

@{
    ViewBag.Title = "Listar";
}

<h2>Listar</h2>

<table >
    <thead>
        <tr>
            <th>Nome</th>
            <th>Idade</th>
            <th>Credito</th>
            <th>Necessidades Especiais</th>
            <th>Data de Nascimento</th>
        </tr>
    </thead>
    <tbody>
    @foreach (var item  in Model)
    {
         <tr>
             <td>@item.nome</td>
             <td>@item.idade</td>
             <td>@item.credito</td>
             <td>@item.NecessidadesEspeciais</td>
             <td>@item.Nascimento</td>
         </tr>
    }
    </tbody>
</table>



